&run_setup
        lattice = lattice.lte,
        default_order = 1,
        use_beamline = bl_nopars,
        p_central_mev = 500.0,
        final = %s.fin,
        print_statistics = 1,
        element_divisions=10
&end

&twiss_output
        filename=%s.twi,
        matched=0,
        beta_x=4,
        alpha_x=1.5
&end

&run_control
        n_indices = 0
&end

&bunched_beam
        emit_nx=1e-6,
        emit_ny=1e-6,
        momentum_chirp=5,
        use_twiss_command_values=1,
        n_particles_per_bunch = 50000,
        sigma_dp = 0.001,
        sigma_s = 650e-6,
        distribution_type[0] = 3*"gaussian",
        distribution_cutoff[0] = 3*3,
        enforce_rms_values[0] = 1,1,1
&end

&track &end
