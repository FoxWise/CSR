
sddssequence M_pars.sdds -define=psi -sequence=begin=0.0,number=71,delta=0.1

sddsprocess M_pars.sdds -define=parameter,beta1,17.6,units=m \
        -define=parameter,alpha1,0.5,units=m \
        -define=parameter,beta2,20.0,units=m \
        -define=parameter,alpha2,0.9,units=m \
        -define=column,r11,"beta2 beta1 / sqrt psi cos psi sin alpha1 mult + mult" \
        -define=column,r12,"beta2 beta1 mult sqrt psi sin mult" \
        -define=column,r21,"1 beta2 beta1 mult sqrt / psi cos alpha1 alpha2 - mult psi sin 1 alpha1 alpha2 mult + mult - mult" \
        -define=column,r22,"beta1 beta2 / sqrt psi cos psi sin alpha2 mult - mult"

exec elegant phase_adv.ele | tee phase_adv.log



